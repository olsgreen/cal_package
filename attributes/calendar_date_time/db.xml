<?xml version="1.0" encoding="UTF-8"?>
<schema
  xmlns="http://www.concrete5.org/doctrine-xml/0.5"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">

  <table name="atCalendarDateTime">

    <field name="avID" type="integer">
      <unsigned/>
      <key/>
    </field>

    <field name="is_all_day" type="integer">
      <unsigned/>
      <default value="0"/>
    </field>

     <field name="is_multi_day" type="integer">
      <unsigned/>
      <default value="0"/>
    </field>

    <field name="date_from" type="datetime"/>

    <field name="date_to" type="datetime"/>

  </table>
</schema>